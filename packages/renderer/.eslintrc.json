{
  "env": {
    "browser": true,
    "node": false
  },
  "parser": "vue-eslint-parser",
  "extends": [
    "eslint:recommended"
    /** @see https://github.com/typescript-eslint/typescript-eslint/tree/master/packages/eslint-plugin#recommended-configs */
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
